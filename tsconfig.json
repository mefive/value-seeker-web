{
  "compilerOptions": {
    "target": "es5",
    "moduleResolution": "node",
    "allowJs": true,
    "noEmit": true,
    "strict": true,
    "strictNullChecks": false,
    "isolatedModules": true,
    "esModuleInterop": true,
    "jsx": "react",
    "baseUrl": ".",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "types": ["reflect-metadata"],
    "paths": {
      "types": ["./src/app/types"]
    }
  },
  "include": [
    "./src"
  ]
}
